{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    },
    "metrics": {
      "enabled": true,
      "buckets": [
        0.1,
        0.5,
        1,
        5,
        15,
        50,
        250,
        500
      ]
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "app": {
    "projectName": "default"
  },
  "application": {
    "isolationLevel": "SERIALIZABLE",
    "transactionRetryPolicy": {
      "enabled": false
    }
  },
  "db": {
    "type": "postgres",
    "host": "localhost",
    "port": 5432,
    "username": "postgres",
    "password": "postgres",
    "enableSslAuth": false,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    },
    "database": "postgres",
    "schema": "osm_sync_tracker"
  }
}
